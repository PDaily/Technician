<h1>Listing ticket_boxes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_boxes.each do |ticket_box| %>
      <tr>
        <td><%= link_to 'Show', ticket_box %></td>
        <td><%= link_to 'Edit', edit_ticket_box_path(ticket_box) %></td>
        <td><%= link_to 'Destroy', ticket_box, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket box', new_ticket_box_path %>
